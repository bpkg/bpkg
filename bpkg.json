{
  "name": "bpkg",
  "version": "1.0.7",
  "description": "Lightweight bash package manager",
  "global": true,
  "repo": "bpkg/bpkg",
  "install": "bash setup.sh",
  "commands": {
    "lint": "command shellcheck **/*.sh",
    "list-sources": "echo ${BPKG_SCRIPT_SOURCES[@]} | sed 's/ /\n/g'"
  }
}
